{
    "name": "delaneymethod/craft-mix",
    "description": "Helper plugin for Laravel Mix in Craft CMS templates.",
    "version": "4.0.0",
    "type": "craft-plugin",
    "license": "MIT",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "yii2",
        "craft-plugin",
        "mix",
        "craft-mix",
        "craft4",
        "laravel",
        "delaneymethod"
    ],
    "support": {
	    "email": "hello@delaneymethod.com",
		"issues": "https://github.com/delaneymethod/craft-mix/issues?state=open",
		"source": "https://github.com/delaneymethod/craft-mix",
		"docs": "https://github.com/delaneymethod/craft-mix",
		"rss": "https://github.com/delaneymethod/craft-mix/commits/v4.atom"
    },
    "authors": [
        {
            "name": "DelaneyMethod",
            "email": "hello@delaneymethod.com",
            "homepage": "http://www.delaneymethod.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "craftcms/cms": "^4.0.0-beta.2"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^5.4",
        "codeception/module-yii2": "^1.1",
        "craftcms/rector": "dev-main"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "delaneymethod\\craftMix\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Mix",
        "handle": "craft-mix",
        "schemaVersion": "4.0.0",
        "documentationUrl": "https://github.com/delaneymethod/craft-mix/blob/v4/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/delaneymethod/craft-mix/v4/CHANGELOG.md",
        "downloadUrl": "https://github.com/delaneymethod/craft-mix/archive/v4.zip",
        "hasCpSettings": true,
        "hasCpSection": false,
        "components": {
            "craftMix": "delaneymethod\\craftMix\\services\\CraftMixService"
        },
        "class": "delaneymethod\\craftMix\\CraftMix"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
